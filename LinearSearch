#include<stdio.h>
int FindElement(int a[],int sz,int element);

int main(){
    int n;///size
    scanf("%d",&n);
    int arr[n];///1,2,3,4,5,6;
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    int element;///7
    scanf("%d",&element);
    int pos = FindElement( arr, n,element);
    if(pos == -1){
        printf("Not Found!");
    }
    else{
        printf("Found at Position %d\n",pos);
    }
}
int FindElement(int a[],int sz,int element){

    for(int i=0;i<sz;i++){
        if(a[i]==element){
            return i;
        }
    }
    return -1;

}
